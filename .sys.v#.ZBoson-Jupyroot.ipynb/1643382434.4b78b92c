   "source": [
    "# Z Boson Analysis using a Boosted Decision Tree, with TMVA and JupyROOT"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "726c3cc0",
   "metadata": {},
   "outputs": [],
   "source": [
    "import ROOT\n",
    "from ROOT import TMath, TChain\n",
    "import time\n",
    "import uproot3\n",
    "import numpy as np\n",
    "import root_pandas as rp"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "6f09a8d2",
